all: msvc_release

clean: clean_msvc_release

before_msvc_release: 
	cmd /c if not exist .cmp\\msvc\\bin\\Release md .cmp\\msvc\\bin\\Release
	cmd /c if not exist $(OBJDIR_MSVC_RELEASE) md $(OBJDIR_MSVC_RELEASE)
	cmd /c if not exist $(OBJDIR_MSVC_RELEASE)\\sqlite3 md $(OBJDIR_MSVC_RELEASE)\\sqlite3
	cmd /c if not exist $(OBJDIR_MSVC_RELEASE)\\lzmalib md $(OBJDIR_MSVC_RELEASE)\\lzmalib
	cmd /c if not exist $(OBJDIR_MSVC_RELEASE)\\miniz   md $(OBJDIR_MSVC_RELEASE)\\miniz

after_msvc_release: 
#	$(CPDE_USR)/bin/cpde_usr -dll -project=$(PROJECT_NAME)  -root=$(PROJECT_DIR)  -build=$(TARGET_NAME)  -target=$(TARGET_OUTPUT_FILE)  -usr=$(CPDE_USR)

