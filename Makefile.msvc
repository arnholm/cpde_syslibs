WORKDIR = %cd%

CC0 = cl.exe
CPP0 = cl.exe
LIB0 = link.exe
LD0 = link.exe
WINDRES0 = rc.exe

INC0 = /I$(BOOST_INCLUDE) /I$(MSGPACK_INCLUDE)
CFLAGS0 = -DOP_LITE_IMPLEMENTATION -DSQLITE_ENABLE_COLUMN_METADATA
RESINC0 = 
LIBDIR0 = 
LIB0 = 
LDFLAGS0 = 

INC_MSVC_RELEASE = $(INC0)
CFLAGS_MSVC_RELEASE = $(CFLAGS0) /MD /EHs /GR /GF /Ox /W3 /EHsc /MD /DWIN32 /D__NO_VC_CRTDBG__ /D_UNICODE /D_WINDOWS /GR /EHsc  /D_CRT_SECURE_NO_WARNINGS /D_CRT_NONSTDC_NO_DEPRECATE /D_CRT_SECURE_DEPRECATE
